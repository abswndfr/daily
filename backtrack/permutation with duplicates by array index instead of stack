N = 3
M = 2 
rs = [0] * M

def recur(num, d):
    if num == M:
        print(rs)
        return

    for i in range(1, N+1):
        #rs.append(i)
        rs[d] = i
        recur(num+1, d+1)
        #rs.pop()						# backtrack

recur(0, 0)      # [1, 1] [1, 2]	[1, 3]	[2, 1]	[2, 2]	[2, 3]	[3, 1]	[3, 2]	[3, 3]
